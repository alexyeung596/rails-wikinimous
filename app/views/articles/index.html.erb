<h1>Articles</h1>

<div class="btn btn-warning">
  <%= link_to 'Create New Article', new_article_path %>
</div>

<div class="row">
  <ul>
    <% @articles.each do |article| %>
      <li class="col-sm-8 col-sm-offset-2">
        <%= link_to article.title, article %>
      </li>
    <% end %>
  </ul>
</div>
